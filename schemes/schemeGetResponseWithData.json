{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "status": "success",
            "data": {
                "id": 1,
                "employee_name": "Tiger Nixon",
                "employee_salary": 320800,
                "employee_age": 61,
                "profile_image": ""
            },
            "message": "Successfully! Record has been fetched."
        }
    ],
    "required": [
        "status",
        "data",
        "message"
    ],
    "properties": {
        "status": {
            "$id": "#/properties/status",
            "type": "string",
            "title": "The status schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "success"
            ]
        },
        "data": {
            "$id": "#/properties/data",
            "title": "The data schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "id": 1,
                    "employee_name": "Tiger Nixon",
                    "employee_salary": 320800,
                    "employee_age": 61,
                    "profile_image": ""
                }
            ],
            "required": [
                "id",
                "employee_name",
                "employee_salary",
                "employee_age",
                "profile_image"
            ],
            "properties": {
                "id": {
                    "$id": "#/properties/data/properties/id",
                    "type": "integer",
                    "title": "The id schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": 0,
                    "examples": [
                        1
                    ]
                },
                "employee_name": {
                    "$id": "#/properties/data/properties/employee_name",
                    "type": "string",
                    "title": "The employee_name schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "Tiger Nixon"
                    ]
                },
                "employee_salary": {
                    "$id": "#/properties/data/properties/employee_salary",
                    "type": "integer",
                    "title": "The employee_salary schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": 0,
                    "examples": [
                        320800
                    ]
                },
                "employee_age": {
                    "$id": "#/properties/data/properties/employee_age",
                    "type": "integer",
                    "title": "The employee_age schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": 0,
                    "examples": [
                        61
                    ]
                },
                "profile_image": {
                    "$id": "#/properties/data/properties/profile_image",
                    "type": "string",
                    "title": "The profile_image schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        ""
                    ]
                }
            },
            "additionalProperties": true
        },
        "message": {
            "$id": "#/properties/message",
            "type": "string",
            "title": "The message schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Successfully! Record has been fetched."
            ]
        }
    },
    "additionalProperties": true
}